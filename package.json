{
  "name": "multistep-checkout",
  "version": "1.0.0",
  "description": "single-page app that simulates a checkout experience",
  "main": "./client/index.jsx",
  "scripts": {
    "start:dev": "nodemon server/index.js",
    "build:dev": "webpack -d --watch",
    "seed:dev": "mysql -u root -p < db/schema.sql",
    "dev": "concurrently \"npm run start:dev\" \"npm run build:dev\" \"npm run seed:dev\""
  },
