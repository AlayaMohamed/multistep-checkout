{
  "name": "multistep-checkout",
  "version": "1.0.0",
  "description": "single-page app that simulates a checkout experience",
  "main": "./client/index.jsx",
  "scripts": {
    "start:dev": "nodemon server/index.js",
    "build:dev": "./node_modules/.bin/babel client --watch --out-dir public --presets @babel/preset-react --source-maps inline",
    "seed:dev": "mysql -u root < db/schema.sql",
    "dev": "concurrently \"npm run start:dev\" \"npm run build:dev\" \"npm run seed:dev\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/underthecode/multistep-checkout.git"
  },
  "author": "underthecode",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/underthecode/multistep-checkout/issues"
  },
  "homepage": "https://github.com/underthecode/multistep-checkout#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "concurrently": "^4.1.1",
    "express": "^4.17.1",
    "mysql": "^2.17.1",
    "nodemon": "^1.19.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.4",
    "@babel/preset-react": "^7.0.0"
  }
}
